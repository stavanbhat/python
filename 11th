def calculation(a, b):
    mean = (a*b)/(a+b)
    print(mean)
def bigger(a, b):
    if(a>b):
        print("first number is greterthan second")
    else:
        print("second number is greterthan first")
a = 9
b = 8
bigger(a,b)
calculation(a, b)
c = 6
d = 7
bigger(c, d)
calculation(c, d)