num = int(input("Enter a number"))
while(num <= 21):
    num = int(input("Enter a number"))
    print(num)

i = int(input("Enter a number"))
while( i >= 0):
    print(i)
    i = i - 1

a = int(input("Enter a number"))
while( a <= 32):
    print(a)
    a = a + 1 

c = input("Enter a string: ")
for i in range(5) :
    print(c)